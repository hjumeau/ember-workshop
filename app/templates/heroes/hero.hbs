
<div class="row">
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img {{bind-attr src=model.thumbnailUrl}} {{bind-attr alt=model.name}} class="img-thumbnail">
      <div class="caption">
        <h3>{{model.name}}</h3>
        <p>{{model.description}}</p>
        <p>{{#link-to 'heroes.hero.rate' class="btn btn-primary"}}Edit{{/link-to}} <a href="#" class="btn btn-danger" role="button">Cancel</a></p>
      </div>
    </div>
  </div>
</div>
